a=list(input())
b=list(input())
for i in a[:]:
    if i in b:
        a.remove(i)
        b.remove(i)
c=len(a)+len(b)
print(a)
print(b)
print(c)
d=['f','l','a','m','e','s']
while(len(d)>1):
    x=c% len(d)
    d.pop(x-1)
n=d[0]
print(n)
e={'f':'friend','l':'lover','a':'attraction','m':'marriage','e':'enemy','s':'sister'}
print(e[n])
